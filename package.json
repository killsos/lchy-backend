{
  "name": "app-roi-tracker",
  "version": "1.0.0",
  "description": "App ROI数据跟踪系统",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:debug": "node --inspect-brk -r ts-node/register src/server.ts",
    "dev:inspect": "node --inspect -r ts-node/register src/server.ts",
    "watch": "tsc -w",
    "dev:watch": "nodemon --exec ts-node src/server.ts",
    "dev:watch:debug": "nodemon --inspect-brk --exec ts-node src/server.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "generate:seed": "npx sequelize-cli seed:generate --name",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.2",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.8",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.8",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "sequelize",
    "typescript",
    "roi",
    "app",
    "tracking"
  ],
  "author": "Your Name",
  "license": "MIT"
}
