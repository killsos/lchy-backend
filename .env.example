# .env.example
# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=your_password
DB_NAME=app_roi_db
DB_DIALECT=mysql

# 数据库连接池配置
DB_POOL_MAX=20
DB_POOL_MIN=5
DB_POOL_ACQUIRE=60000
DB_POOL_IDLE=10000
DB_POOL_EVICT=1000
DB_CONNECT_TIMEOUT=10000
DB_ACQUIRE_TIMEOUT=60000
DB_QUERY_TIMEOUT=30000

# 数据库SSL配置 (生产环境推荐)
DB_SSL=false
DB_SSL_REJECT_UNAUTHORIZED=true

# 环境配置
NODE_ENV=development
PORT=3000

# 日志配置
LOG_LEVEL=debug

# CORS配置
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080